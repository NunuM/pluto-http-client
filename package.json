  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/node": "18.17.0",
    "@types/benchmark": "^2.1.2",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "benchmark": "^2.1.4"
  }
}
